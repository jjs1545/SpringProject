<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC ".//mybatis.org//DTD Mapper 1.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="category">

	<select id="selectAll" resultType="categoryvo">
		<![CDATA[
			SELECT *
			FROM category
			ORDER BY no desc		
		]]>
	</select>
</mapper>